{
  "type": "object",
  "required": [
      "name",
      "description",
      "founder",
      "isPublic"
  ],
  "properties": {
      "name": {
          "type": "string",
          "description": "Name of the club",
          "maxLength": 30
      },
      "description": {
          "type": "string",
          "description": "Description of the club"
      },
      "founder": {
          "type": "string",
          "description": "Username of the founder of the club",
          "maxLength": 30
      },
      "bannerImgUrl": {
          "type": "string",
          "description": "URL to access the club's banner image at. Should be jpg, png, or gif, and accessible via http.",
          "pattern": "^https*://.+/.+(.jpg|.jpeg|.png|.gif)"
      },
      "isPublic": {
          "type": "boolean",
          "description": "Whether or not the club is open to the public."
      }
  },
  "additionalProperties": false
}